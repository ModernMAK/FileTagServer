<!DOCTYPE HTML>
<html>
<head>
    <title>{{TITLE}}</title>
    <meta charset='utf-8'>
    <link rel="top" title="{{TITLE}}" href="/">
    <link class="main-stylesheet" href="/stylesheets/common.css" media="screen" rel="stylesheet" type="text/css">
</head>
<body>
<div id="header">
    <a href="/">Main Page</a>
    <a href="/show/image/index">Images</a>
    <a href="/show/tag/index">Tags</a>
    <a href="/upload/image">Upload</a>
</div>
<div id="content">
    <div id="post-view">
        <div class="content" id="right-col">
            <div>
                <img alt="{{IMG_ALT}}" height="{{IMG_HEIGHT}}" width="{{IMG_WIDTH}}"
                     src="/images/posts/{{IMG_ID}}/hirez.{{IMG_EXT}}">
            </div>
        </div>
        <div>
            <b>Tag List</b>
            <ul>
                {{#TAG_LIST}}
                <li><a href="/show/tag/{{TAG_ID}}">{{TAG_NAME}}</a></li>
                {{/TAG_LIST}}
                {{^TAG_LIST}}
                <li>No Tags Present</li>
                {{/TAG_LIST}}
            </ul>
            <a href="/show/image/{{IMG_ID}}/edit">Edit Tags</a>
        </div>
    </div>
</div>
</body>
</html>
