<!DOCTYPE HTML>
<html>
<head>
    <title>{{TITLE}}</title>
    <meta charset='utf-8'>
    <link rel="top" title="{{TITLE}}" href="/">
    <link class="main-stylesheet" href="/stylesheets/common.css" media="screen" rel="stylesheet" type="text/css">
</head>
<body>
<div id="header">
    <a href="/">Main Page</a>
    <a href="/show/image/index">Images</a>
    <a href="/upload/image">Upload</a>
</div>
<div id="content">
    <div id="post-view">
        <div class="content" id="right-col">
            {{#IMG_LIST}}
            <a href="/show/image/{{IMG_ID}}">
                <div style="width: 256px; height: 256px; display: inline; vertical-align: middle; horiz-align: center">
                    <img alt="{{IMG_ALT}}" src="/images/posts/{{IMG_ID}}/thumb.{{IMG_EXT}}"
                         style="margin-left: auto; margin-right: auto">
                </div>
            </a>
            {{/IMG_LIST}}
        </div>
    </div>
</div>
</body>
</html>
